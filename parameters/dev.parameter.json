{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "location": {
        "value": "EastUS"
      },
      "appServicePlanName": {
        "value": "myAppServicePlan"
      },
      "webAppName": {
        "value": "myWebApp"
      },
      "containerRegistryName": {
        "value": "DianaContainerRegistry"
      },
      "containerRegistryImageName": {
        "value": "myImage"
      },
      "containerRegistryImageVersion": {
        "value": "latest"
      },
      "sku": {
        "value": {
          "capacity": 1,
          "family": "B",
          "name": "B1",
          "size": "B1",
          "tier": "Basic"
        }
      },
      "appSettingsKeyValuePairs": {
        "value": {
          "WEBSITES_ENABLE_APP_SERVICE_STORAGE": false,
          "DOCKER_REGISTRY_SERVER_URL": "https://DianaContainerRegistry.azurecr.io",
          "DOCKER_REGISTRY_SERVER_USERNAME": "your-username",
          "DOCKER_REGISTRY_SERVER_PASSWORD": "your-password"
        }
      }
    }
  }
  